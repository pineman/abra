{
  "name": "abra",
  "version": "1.0.0",
  "description": "Race against others in this new exciting typing game™! https://abra.pineman.eu",
  "keywords": [
    "game",
    "typing"
  ],
  "main": "index.html",
  "scripts": {
    "build": "cd client && ./build.sh",
    "watch": "npm-watch build"
  },
  "watch": {
    "build": {
      "patterns": [
        "js",
        "less",
        "index.html"
      ],
      "extensions": "js,less,html"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pineman/abra.git"
  },
  "type": "module",
  "contributors": [
    {
      "name": "Diogo Almiro",
      "email": "diogoalmiro@gmail.com",
      "url": "http://diogoalmiro.com"
    },
    {
      "name": "João Pinheiro",
      "email": "joao.castropinheiro@gmail.com",
      "url": "https://pineman.eu"
    },
    {
      "name": "Diogo Tito",
      "email": "diogotitomarques@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pineman/abra/issues"
  },
  "homepage": "https://github.com/pineman/abra#readme",
  "devDependencies": {
    "browserify": "17.0.0",
    "html-minifier": "4.0.0",
    "less": "4.1.2",
    "less-plugin-clean-css": "1.5.1",
    "npm-watch": "0.11.0"
  },
  "dependencies": {
    "express": "4.18.1",
    "express-ws": "5.0.2"
  }
}
